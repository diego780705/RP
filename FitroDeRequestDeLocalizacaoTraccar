var latitude = msg.payload[0].latitude;
var longitude = msg.payload[0].longitude;
var bateria = msg.payload[0].attributes.batteryLevel;

msg.payload = {
    "command": {
        "draggable": true,
        "zoom": 16,
        "name": "Diego",    
        "lat": latitude,
        "lon": longitude,
        "radius": 10,   
        "iconColor": '#000000'   
    } 
};

msg.bateria = bateria;

return msg;
